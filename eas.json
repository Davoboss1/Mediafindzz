{
  "cli": {
    "version": ">= 3.8.1"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "ios": {
        "resourceClass": "m-medium"
      }
    },
    "preview": {
      "distribution": "internal",
      "ios": {
        "resourceClass": "m-medium"
      }
    },
    "preview1": {
        "android" : {
            "buildType": "apk"
        }
    },
    "preview2": {
        "android" : {
            "gradleCommand": ":app:assembleRelease"
        }
    },
    "preview3": {
        "android" : {
            "buildType": "apk",
            "developmentClient": true
        }
    },
    "production": {
      "ios": {
        "resourceClass": "m-medium"
      },
      "android" : {
        "buildType": "apk"
    }
    }
  },
  "submit": {
    "production": {}
  }
}
